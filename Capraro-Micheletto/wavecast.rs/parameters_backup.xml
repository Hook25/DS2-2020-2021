<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="messagesPerTick" displayName="Max number of messages a Relay can send per Tick" type="int" 
						defaultValue="500" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="waveGenerationLastPeriod" displayName="Last period for waves generation" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="waveGenerationDistribution" displayName="Wave generation distribution" type="string" 
						defaultValue="UNIFORM" 
						isReadOnly="false" 
						values="UNIFORM NORMAL EXPONENTIAL"
											/>
				<parameter name="waveGenerationDistributionParam1" displayName="Wave generation distribution mean, lambda or left bound" type="double" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="waveGenerationDistributionParam2" displayName="Wave generation distribution variance or right bound if applicable" type="double" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="loadColorScale" displayName="Load color scale" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="tickAccuracy" displayName="Tick accuracy" type="double" 
						defaultValue="0.01" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="relayJoinMaxPeriod" displayName="Last period for relays to join" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="relayJoinProbability" displayName="Probability that [0,Max] relays join every period" type="double" 
						defaultValue="0.1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="relaysNumber" displayName="Number of relays" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="lossProbability" displayName="Wave loss probability" type="double" 
						defaultValue="0.01" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="wavePropagationDistance" displayName="Wave Propagation Distance" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="latencyRatio" displayName="Latency ratio (% of period)" type="double" 
						defaultValue="0.05" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="relayJoinMaxNumber" displayName="Max number of joining relays (per period)" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="spaceHeight" displayName="Space Height" type="int" 
						defaultValue="50" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="spaceWidth" displayName="Space width" type="int" 
						defaultValue="50" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
		</parameters>


