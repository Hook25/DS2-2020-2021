<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="true" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="p2p_probability" displayName="(F) - P2P Probability" type="double" 
						defaultValue="0.25" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="topology" displayName="(B) - Topology" type="java.lang.String" 
						defaultValue="random" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
												values="random ring star star+ "
											/>
				<parameter name="spawn_probability" displayName="(I) - Spawn probability (only for random topology)" type="double" 
						defaultValue="0.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="broadcast_probability" displayName="(H) - Broadcast Probability" type="java.lang.String" 
						defaultValue="1 - (P2P Probability + PubSub Probability)" 
						isReadOnly="true" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
											/>
				<parameter name="kill_probability" displayName="(J) - Kill probability (only for random topology)" type="double" 
						defaultValue="0.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="perturbation_probability" displayName="(E) - Perturbation Probability" type="double" 
						defaultValue="0.02" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
												values="0.01 0.02 0.05 0.1 0.2 0.3 "
											/>
				<parameter name="pubsub_probability" displayName="(G) - PubSub Probability" type="double" 
						defaultValue="0.1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="relays_count" displayName="(A) - Relays Count" type="int" 
						defaultValue="30" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="spread_speed" displayName="(C) - Perturbation Spread Speed" type="double" 
						defaultValue="0.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
											/>
				<parameter name="message_size" displayName="(D) - Message Size (KB)" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
		</parameters>


